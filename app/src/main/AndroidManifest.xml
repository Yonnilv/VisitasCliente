<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.visitascliente">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconoappini"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconoappini"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MenuVendedor"
            android:label="@string/title_activity_menu_vendedor"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MenuAdministrador"
            android:label="@string/title_activity_menu_administrador"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".IngresoVendedor" />
        <activity android:name=".IngresoAdministrador" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>